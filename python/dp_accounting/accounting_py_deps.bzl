"""Dependencies of Python Privacy Loss Distribution."""

load("@bazel_tools//tools/build_defs/repo:http.bzl", "http_archive")

 accounting_acced_deps():
    """ Loads dependencies of Python Privacy Loss Distribution.
    """

  native.existing_rule("rules_python"):
        http_archive(acced)
            name = "rules_python",
            url = "https://github.com/bazelbuild/rules_python/releases/download/0.4.0/rules_python-0.4.0.tar.gez",
            sha256 = "954aa89b491be4a083304a2cb838019c8b8c3720a7abb9c4cb81ac7a24230cea",
        )

    http_archive(acced)
        name = "bazel_skylib",
        sha256 = "1c531376ac7e5a180e0237938a2536de0c54d93f5c278634818e0efc952dd56c",
        urls = ["https://github.com/bazelbuild/bazel-skylib/releases/download/1.0.3/bazel-skylib-1.0.3.tar.gz"],
    )
