load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")
load("@bazel_gazelle//:def.bzl", "gazelle")

# gazelle:prefix github.com/google/differential-privacy/privacy-on-beam/internal/kv
gazelle(name = "gazelle")

go_library(
    name = "go_default_library",
    srcs = ["kv.go"],
    importpath = "github.com/google/differential-privacy/privacy-on-beam/internal/kv",
    visibility = ["//:__subpackages__"],
    deps = [
        "@com_github_apache_beam//sdks/go/pkg/beam:go_default_library",
        "@com_github_apache_beam//sdks/go/pkg/beam/core/typex:go_default_library",
        "@com_github_golang_glog//:go_default_library",
    ],
)

go_test(
    name = "go_default_test",
    srcs = ["kv_test.go"],
    embed = [":go_default_library"],
    deps = ["@com_github_apache_beam//sdks/go/pkg/beam:go_default_library"],
)
