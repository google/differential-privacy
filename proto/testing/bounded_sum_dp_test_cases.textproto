bounded_sum_dp_test_case {
  name: "Gaussian noise, empty sum, default parameters"
  dp_test_parameters {
    epsilon: 1.09861228866810969140  # = log(3)
    delta: 0.00001
    delta_tolerance: 0.00117
    granularity: 0.0078125
  }
  bounded_sum_sampling_parameters {
    number_of_samples: 1000000
    noise_type: GAUSSIAN
    max_partitions_contributed: 1
    lower_bound: 0.0
    upper_bound: 1.0
    epsilon: 1.09861228866810969140  # = log(3)
    delta: 0.00001
    # No raw_entry fields because the sum is empty. The number of extra entries
    # contained in the neighbouring data set equals the square root of the
    # maximum number of partitions a single privacy unit can contribute to. The
    # entries are set to a value that significantly exceeds the bound of larger
